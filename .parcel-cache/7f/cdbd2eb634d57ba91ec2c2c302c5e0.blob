:root {
  --default-font-family: -apple-system, BlinkMacSystemFont, "Segoe UI",
    "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "Helvetica Neue",
    Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji",
    "Segoe UI Symbol";
  --black: #000;
  --white: #fff;
  --gp-green: #66cc00;
  --orange: #ff9500;
  --gp-dark-blue: #292f47;
  --gray: #d9d9d9;
  --light-gray: #f5f5f5;
}

.enform__wrapper {
  display: none;
}

.page-header-image {
  background-color: var(--gp-dark-blue);
}

.footer {
  background-color: #1a1c26;
  color: #fff;
}